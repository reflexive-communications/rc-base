<?xml version="1.0"?>
<extension key="rc-base" type="module">
    <file>rc_base</file>
    <name>RC Base</name>
    <description>
        This extension does nothing by itself, it contains tools, libraries and shared components only.
    </description>
    <license>AGPL-3.0</license>
    <maintainer>
        <author>Sandor Semsey</author>
        <email>sandor@es-progress.hu</email>
    </maintainer>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/rc-base</url>
        <url desc="Documentation">https://github.com/reflexive-communications/rc-base</url>
        <url desc="Support">https://github.com/reflexive-communications/rc-base/issues</url>
        <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2021-06-21</releaseDate>
    <version>0.9.0</version>
    <develStage>alpha</develStage>
    <compatibility>
        <ver>5.0</ver>
    </compatibility>
    <classloader>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <civix>
        <namespace>CRM/RcBase</namespace>
    </civix>
</extension>
