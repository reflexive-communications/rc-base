<?xml version="1.0"?>
<extension key="rc-base" type="module">
    <file>rc_base</file>
    <name>RC Base</name>
    <description>This extension does nothing by itself, it contains tools, libraries and shared components</description>
    <license>AGPL-3.0</license>
    <authors>
        <author>
            <name>Sandor Semsey</name>
            <email>sandor@es-progress.hu</email>
            <role>Maintainer</role>
        </author>
    </authors>
    <urls>
        <url desc="Main Extension Page">https://github.com/reflexive-communications/rc-base</url>
        <url desc="Documentation">https://github.com/reflexive-communications/rc-base</url>
        <url desc="Support">https://github.com/reflexive-communications/rc-base/issues</url>
        <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
    </urls>
    <releaseDate>2026-02-01</releaseDate>
    <version>1.63.2</version>
    <develStage>stable</develStage>
    <compatibility>
        <ver>5.76</ver>
    </compatibility>
    <classloader>
        <psr0 prefix="CRM_" path=""/>
        <psr4 prefix="Civi\" path="Civi"/>
    </classloader>
    <mixins>
        <mixin>scan-classes@1</mixin>
    </mixins>
    <upgrader>CiviMix\Schema\RcBase\AutomaticUpgrader</upgrader>
    <civix>
        <namespace>CRM/RcBase</namespace>
        <format>25.01.1</format>
    </civix>
</extension>
